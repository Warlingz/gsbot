function CheckCubeStaff()
{
	var cube = me.GetItems(549);
	var staff = me.GetItems(92);
	var ammy = me.GetItems(521);
	var _result = false;
	if(cube.length > 0 && ammy.length > 0 && staff.length > 0){
		NTR_CubeStaff();
		_result = true;
	}
	return _result;
}
