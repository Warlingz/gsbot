				NTC_Delay(1000);
				NTC_SendMsgToScript("Manager.ntj", "SCRIPT_END");	
			}
		 }
		 else if(msg.indexOf("timeout") != -1 && msg.indexOf(NTConfig_Leader) != -1){
			if(!rushComplete && !nospam){
				var rusheepos = NTR_RetrieveRusheePos();
				NTC_Delay(200);		
				var pos = (parseInt(rusheepos) + 1);
				var _area = GetArea();
				NTC_Delay(1000);	
