// Main Script Function ---------------------------------------------
function Questing(area)
{	
	switch(area){
		case 0 :
			NTC_Delay(500);
			break;
		case 1 : // ACT 1 ---------------------------------------------
			NTC_Delay(1000);		
			if(NTC_InTown() && !LocationChecka1){			
				NTR_CheckLocation();
				LocationChecka1 = true;
				return LocationChecka1;
			}
			break;
			
		case 8 :
			NTC_Delay(1000);
			NTR_ClearLevel(false);
			break;
		case 40 : // ACT 2 ---------------------------------------------
			NTC_Delay(1000);		
			if(NTC_InTown() && !LocationChecka2){
				NTR_CheckLocation();
				LocationChecka2 = true;
				return LocationChecka2;
			}
			break;			
		case 75 : // ACT 3 ---------------------------------------------
			NTC_Delay(1000);		
			if((!me.GetQuest(22,12) && !me.GetQuest(22,0)) && !LocationChecka3){
				NTR_CheckLocation();
				LocationChecka3 = true;
				return LocationChecka3;
			}
			if( NTC_InTown() && !LocationChecka3){
				NTR_CheckLocation();
				LocationChecka3 = true;
				return LocationChecka3;
			}
			break;				
		case 103 : // ACT 4 ---------------------------------------------
			NTC_Delay(1000);		
			break;	
		case 109 : // ACT 5 ---------------------------------------------
			NTC_Delay(1000);		
			break;	
		default :
			NTC_Delay(500);
			break;
	}
	return false;
}

